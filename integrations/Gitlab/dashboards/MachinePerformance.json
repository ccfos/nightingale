          "h": 3,
          "w": 5,
          "x": 0,
          "y": 0,
          "i": "a4389d18-8aed-4207-8fa9-4b25da036d6a",
          "isResizable": true
        },
        "version": "3.0.0",
        "datasourceCate": "prometheus",
        "datasourceValue": "${DS_PROMETHEUS}",
        "targets": [
          {
            "refId": "B",
            "expr": "node_boot_time_seconds{instance=~\"$instance\"} / 60 / 60 /24 / 30 / 365",
            "legend": "Uptime"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {}
          }
        ],
        "name": "Uptime",
        "links": [],
        "maxPerRow": 4,
        "custom": {
          "textMode": "value",
          "graphMode": "none",
          "colorMode": "value",
