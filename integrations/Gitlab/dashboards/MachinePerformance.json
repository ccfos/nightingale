          "colSpan": 1,
          "textSize": {}
        },
        "options": {
          "valueMappings": [],
          "standardOptions": {
            "util": "none",
            "decimals": 0
          },
          "thresholds": {
            "steps": [
              {
                "color": "#73BF69",
                "value": null,
                "type": "base"
              }
            ]
          }
        }
      },
