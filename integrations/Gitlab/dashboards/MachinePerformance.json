{
  "name": "GitLab - Machine Performance",
  "tags": "",
  "ident": "",
  "configs": {
    "version": "3.0.0",
    "links": [],
    "var": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "definition": "prometheus",
        "defaultValue": 5
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "cate": "prometheus",
          "value": "${DS_PROMETHEUS}"
        },
        "definition": "query_result(node_boot_time_seconds)",
        "reg": "/instance=\"(?<text>[^\"]*)/",
        "multi": false
      }
    ],
    "panels": [
      {
        "type": "stat",
        "id": "a4389d18-8aed-4207-8fa9-4b25da036d6a",
