{
    "name": "ARMS-API",
    "tags": "ARMS JVM",
    "ident": "",
    "configs": {
        "version": "3.0.0",
        "links": [],
        "var": [
            {
                "name": "datasource",
                "type": "datasource",
                "definition": "prometheus"
            },
            {
                "type": "query",
                "name": "service",
                "definition": "label_values(arms_system_cpu_idle,service)",
                "allOption": false,
                "multi": false,
                "reg": "",
                "datasource": {
                    "cate": "prometheus",
                    "value": "${datasource}"
                }
            },
            {
                "type": "query",
                "name": "rpc",
                "definition": "label_values(arms_app_requests_count{service=\"$service\",callType=~\"^dsf$|^http$|^dubbo$|^mq$|^user_method$|^producer$|^thrift$|^hsf$|^server$|^grpc$\",rpc!=\"__all__\"},rpc)",
                "allOption": false,
                "multi": false,
                "reg": "",
                "datasource": {
                    "cate": "prometheus",
                    "value": "${datasource}"
                }
            },
            {
                "type": "query",
                "name": "host",
                "definition": "label_values(arms_system_cpu_idle{service=\"$service\"},host)",
                "allValue": ".*",
                "allOption": true,
                "multi": false,
                "reg": "",
                "datasource": {
                    "cate": "prometheus",
                    "value": "${datasource}"
                }
            }
        ],
        "panels": [
